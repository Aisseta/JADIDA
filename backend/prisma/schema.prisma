generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id_user           Int      @id @default(autoincrement())
  url_photo_profil  String?  // not obligatory
  lastname          String
  firstname         String   
  pseudo            String   @unique
  sexe              String
  email             String   @unique
  password          String
  address           String?  @unique
  city              String?
  description       String?
  
}


